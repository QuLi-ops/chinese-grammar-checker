{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020"],
    "types": ["@cloudflare/workers-types"],
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "outDir": "./worker-dist",
    "esModuleInterop": true
  },
  "include": ["worker/**/*"],
  "exclude": ["node_modules", ".next", "dist"]
} 